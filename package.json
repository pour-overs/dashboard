    "cookie-parser": "^1.4.5",
    "firebase": "^7.14.5",
    "firebase-admin": "^8.12.1",
    "helmet": "^3.22.0",
    "marked": "^2.0.0",
    "polka": "next",
    "sirv": "^0.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
